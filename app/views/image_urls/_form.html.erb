<%= simple_form_for(@image_url, data: { turbo: false }) do |f| %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>
  <div class="form-inputs">
    <%= f.input :url %>
    <%= f.input :order %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
