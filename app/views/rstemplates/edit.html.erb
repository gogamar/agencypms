
<div class="pb-5 mb-5">
  <%= link_to rstemplate_path(@rstemplate), class: "btn btn-light btn-icon-split me-3" do %>
    <span class="icon text-white-600">
        <i class="fas fa-arrow-left"></i>
    </span>
    <span class="text m-0 text-primary" style="font-size: 0.85rem">
      Tornar
    </span>
  <% end %>
  <hr>
  <h4 class="mt-4 font-weight-bold text-primary text-center">Modificar el model de contracte: <%= @rstemplate.title %></h4>
  <hr>
  <div class="pt-4">
    <%= render 'form', rstemplate: @rstemplate %>
  </div>
</div>
