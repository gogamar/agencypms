<section class="theme-bg call-to-act-wrap">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="call-to-act">
          <div class="call-to-act-head">
            <% typedtext = [t("call_to_action_typed_1"), t("call_to_action_typed_2")] %>
            <h3 class="text-secondary"><%= t("call_to_action_title") %></h3>
            <h2 class="text-white"><span><%= t("call_to_action_text_1") %></span> <span data-controller="typed-js" data-typed-js-typedtext-value="<%= typedtext %>"></span></h2>
          </div>
          <%= link_to t("book_online"), list_path, class: "btn btn-call-to-act" %>
        </div>
      </div>
    </div>
  </div>
</section>
